<?xml version="1.0" encoding="utf-8"?>
<com.nsx.sest.mine_views.SSLinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#000000"
    android:gravity="center"
    android:orientation="vertical"
    tools:context=".init_page.InitActivity">

    <TextView
        android:id="@+id/message"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/setup_font"
        android:gravity="center"
        android:text="------------------"
        android:textColor="#FFFFFF" />

    <Button
        android:id="@+id/continue_btn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="5dp"
        android:background="#FFFFFF"
        android:fontFamily="@font/setup_font"
        android:padding="5dp"
        android:text="Continue"
        android:textColor="#000000" />
</com.nsx.sest.mine_views.SSLinearLayout>